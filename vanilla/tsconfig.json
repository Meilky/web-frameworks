{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable"
		],
		"skipLibCheck": true,
		/* Bundler mode */
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		/* me */
		"exactOptionalPropertyTypes": true,
		"baseUrl": "./src",
		"paths": {
			"@actions/*": [
				"actions/*"
			],
			"@models/*": [
				"models/*"
			]
		}
	},
	"include": [
		"src"
	]
}
